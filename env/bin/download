#!/Users/shozendan/Downloads/Bayes_Statistics/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'estat==1.2.1','console_scripts','download'
__requires__ = 'estat==1.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('estat==1.2.1', 'console_scripts', 'download')()
    )
